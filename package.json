{
  "name": "social-server",
  "version": "1.0.0",
  "description": "Backend for Virtual Library UE4",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "debug": "node --inspect index.js",
    "serve": "node index.js",
    "build": "browserify -t vueify -e admin/main.js -o public/bundle.js",
    "build-prod-win": "SET NODE_ENV=production browserify -t vueify -e admin/main.js -o public/bundle.js",
    "build-prod-unix": "NODE_ENV=production browserify -t vueify -e admin/main.js -o public/bundle.js",
    "watch": "watchify -t vueify -e admin/main.js -o public/bundle.js --debug --verbose",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tosh823/social-server"
  },
  "keywords": [
    "node",
    "vue"
  ],
  "author": "Ilya Minyaev",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.17.1",
    "cheerio": "^1.0.0-rc.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "hashids": "^1.1.1",
    "multer": "^1.3.0",
    "nedb": "^1.8.0",
    "node-uuid": "^1.4.8",
    "pug": "^2.0.0-beta11",
    "serve-static": "^1.12.3",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "winston": "^2.3.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "browser": {
    "vue": "vue/dist/vue.common"
  }
}
